<h1>All Pictures</h1>

<% @pictures.each do |picture| %>
  <p><%= link_to(picture.title, "/pictures/#{picture.id}") %>, <%= picture.artist %></p>
<% end %>


<h1>There are <%= @older_pictures.count %> pictures older than a month</h1>

<% @older_pictures.each do |picture| %>
  <p> <%= link_to(picture.title, "/pictures/#{picture.id}") %>, <%= picture.artist %> </p>
<% end %>
